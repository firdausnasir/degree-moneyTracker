<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.expensetracker">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ForgotPassword"
            android:label="Forgot Password"></activity>
        <activity android:name=".HistoryIncome"
            android:label="Income Transaction"/>
        <activity android:name=".HistoryExpense"
            android:label="Expense Transaction"/>
        <activity android:name=".TransactionHistory"
            android:label="Transaction History"/>
        <activity android:name=".Debthomepage"
            android:label="Debt"></activity>
        <activity android:name=".ButtonOwedByMe"
            android:label="List Owed By Me"></activity>
        <activity android:name=".ButtonOwedToMe"
            android:label="List Owed To Me"></activity>
        <activity android:name=".OwedByMe"
            android:label="Add New Debt By Me"></activity>
        <activity android:name=".OwedToMe"
            android:label="Add New Debt To Me"></activity>
        <activity
            android:name=".Income"
            android:label="Income" />
        <activity
            android:name=".Expense"
            android:label="Expense" />
        <activity
            android:name=".Homepage"
            android:label="Homepage" />
        <activity
            android:name=".RegisterActivity"
            android:label="Register" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>